{
  "name": "zksync-ethers",
  "version": "5.8.0",
  "description": "A Web3 library for interacting with the ZkSync Layer 2 scaling solution.",
  "author": {
    "name": "Matter Labs",
    "url": "https://matter-labs.io/",
    "email": "hello@matterlabs.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zksync-sdk/zksync-ethers.git"
  },
  "bugs": {
    "url": "https://github.com/zksync-sdk/zksync-ethers.git/issues"
  },
  "homepage": "https://github.com/zksync-sdk/zksync-ethers.git/tree/ethers-v5#readme",
  "keywords": [
    "zksync",
    "web3",
    "ethereum",
    "layer2"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/",
    "abi/",
    "src/",
    "typechain"
  ],
  "dependencies": {
    "ethers": "~5.7.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^2.0.0",
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.0.0",
    "c8": "^8.0.1",
    "chai": "^4.3.4",
    "ethers": "~5.7.0",
    "mocha": "10.1.0",
    "ncp": "^2.0.0",
    "prettier": "3.0.3",
    "ts-node": "^10.0.0",
    "typechain": "^3.0.0",
    "typedoc": "^0.25.8",
    "typedoc-plugin-markdown": "4.0.0-next.53",
    "typescript": "^5.1.6",
    "gts": "^5.2.0"
  },
  "peerDependencies": {
    "ethers": "~5.7.0"
  },
  "scripts": {
    "test:build": "rm -rf tests/build && tsc -p tsconfig.test.json && cp -r src/typechain/*.d.ts tests/build/src/typechain",
    "test:prepare": "ts-node tests/setup.ts",
    "test:coverage": "c8 -c .nycrc mocha -r ts-node/register --timeout 10000 tests/**/*.test.ts",
    "test:wait": "ts-node tests/wait.ts",
    "test": "mocha -r ts-node/register --timeout 10000 tests/**/*.test.ts",
    "build": "tsc && cp -r src/typechain/*.d.ts build/typechain",
    "lint": "gts lint",
    "lint:fix": "gts fix",
    "watch": "tsc --watch",
    "types:fetch": "cd scripts && ./update-abi.sh && cd ../",
    "types": "rm -rf src/typechain/ typechain && find ./abi -name \"*.json\" -print0 | xargs -0 -I {} typechain --target ethers-v5 --outDir src/typechain {} && ncp src/typechain typechain",
    "clean": "gts clean",
    "docs": "typedoc --out docs --hideInPageTOC true --useCodeBlocks true --includeVersion true  src/index.ts && cd scripts && ./post-process-doc.sh && cd ../"
  },
  "publishConfig": {
    "access": "public"
  }
}
